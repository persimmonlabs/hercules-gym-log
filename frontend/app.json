{
  "expo": {
    "name": "Hercules",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./herc_app_icon.png",
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "entryPoint": "expo-router/entry",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ocreynolds.hercules",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification", "location"],
        "NSLocationWhenInUseUsageDescription": "Hercules uses your location to track your outdoor exercise route, distance, and pace.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Hercules uses your location in the background to continue tracking your outdoor exercise route."
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.ocreynolds.hercules",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBfoPqdk3GoFzs4LFBQhHsx8QzaKN_4ZX4"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./herc_app_icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Hercules uses your location to track your outdoor exercise route, distance, and pace.",
          "locationWhenInUsePermission": "Hercules uses your location to track your outdoor exercise route, distance, and pace."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/images/android-icon-monochrome.png",
          "color": "#FF6B4A",
          "defaultChannel": "workout-reminders",
          "sounds": []
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./herc_app_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#ffffff"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "edb54cbb-52a9-4563-981d-51636d41d1be"
      }
    },
    "owner": "ocreynolds",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/edb54cbb-52a9-4563-981d-51636d41d1be"
    }
  }
}